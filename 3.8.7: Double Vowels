public String doubleVowel(String str) 
{
    String result = "";
    for (int i = 0; i < str.length(); i++) {
        char c = str.charAt(i);
        if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' || 
            c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
            result += "" + c + c;
        } else {
            result += c;
        }
    }
    return result;
}
